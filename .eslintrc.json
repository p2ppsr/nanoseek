{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "only-warn"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "semi": ["warn", "never"],
    "@typescript-eslint/semi": ["warn", "never"],
    "no-extra-semi": "warn"
  }
}
